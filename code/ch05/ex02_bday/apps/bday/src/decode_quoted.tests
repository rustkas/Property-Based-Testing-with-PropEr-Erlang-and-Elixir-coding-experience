decode_quoted_01_test() ->
    String = "\"",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"\"", ""}, Result).

decode_quoted_02_test() ->
    String = "\r\n\"",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"\r\n\"", ""}, Result).

decode_quoted_03_test() ->
    String = "\",\"\"",
    Result = decode_quoted(String),

    ?assertEqual({go_on, "\"\"", "\"\""}, Result).

decode_quoted_04_test() ->
    String = "\",\"\r\n\"",
    Result = decode_quoted(String),
    ?assertEqual({go_on, "\"\"", "\"\r\n\""}, Result).

decode_quoted_05_test() ->
    String = "aaa\"",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"aaa\"", []}, Result).

decode_quoted_06_test() ->
    String = "aaa\",bbb",
    Result = decode_quoted(String),
    ?assertEqual({go_on, "\"aaa\"", "bbb"}, Result).

decode_quoted_07_test() ->
    String = "a\"\r\nbc",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"a\"", "bc"}, Result).

decode_quoted_08_test() ->
    String = "a\"\"a\"\r\nbc",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"a\"a\"", "bc"}, Result).

decode_quoted_09_test() ->
    String = "b\r\nbbb\",\"ccc\"",
    Result = decode_quoted(String),
    ?assertEqual({go_on, "\"b\r\nbbb\"", "\"ccc\""}, Result).

decode_quoted_10_test() ->
    String = "b\"\"bbb\"",
    Result = decode_quoted(String),
    ?assertEqual({done, "\"b\"bbb\"", ""}, Result).
