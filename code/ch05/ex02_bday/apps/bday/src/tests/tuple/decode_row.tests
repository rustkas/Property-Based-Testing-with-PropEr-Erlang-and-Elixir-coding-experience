decode_row_emulate_01_test() ->
    String = "aaa,bbb",
    DecodeRow_2 =
        fun Decode_row(String, Acc) ->
                case decode_field(String) of
                    {go_on, Field, Rest} ->
                        decode_row(Rest, Acc ++ [Field]);
                    {done, Field, Rest} ->
                        Result = Acc ++ [Field],
                        {Result, Rest}
                end
        end,
    DecodeRow_1 =
        fun Decode_row(String) ->
                DecodeRow_2(String, [])
        end,
    Result = DecodeRow_1(String),
    ?assertEqual({["aaa", "bbb"], []}, Result).

decode_row_01_test() ->
    String = "abc,dfg",
    Result = decode_row(String),
    ?assertEqual({["abc", "dfg"], []}, Result).

decode_row_02_test() ->
    String = "\"nik\",\"plik\"",
    Result = decode_row(String),
    ?assertEqual({["\"nik\"", "\"plik\""], []}, Result).
