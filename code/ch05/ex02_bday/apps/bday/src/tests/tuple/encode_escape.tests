    Expected = [$", test, $"],
    ?debugFmt("Expected = ~p~n", [Expected]),
    InputString = Expected,
    Result = escape(InputString),
    ?debugFmt("Result = ~p~n", [Result]).

    %?assertEqual(Expected, Result).

