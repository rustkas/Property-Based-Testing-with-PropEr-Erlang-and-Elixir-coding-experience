decode_unquoted_01_test() ->
    String = "",
    Result = decode_unquoted(String),
    ?assertEqual({done, [], []}, Result).

decode_unquoted_02_test() ->
    String = "\r\n",
    Result = decode_unquoted(String),
    ?assertEqual({done, [], []}, Result).

decode_unquoted_03_test() ->
    String = ",",
    Result = decode_unquoted(String),
    ?assertEqual({go_on, [], []}, Result).

decode_unquoted_04_test() ->
    String = ",\r\n",
    Result = decode_unquoted(String),
    ?assertEqual({go_on, [], [$\r, $\n]}, Result).

decode_unquoted_05_test() ->
    String = "aaa",
    Result = decode_unquoted(String),
    ?assertEqual({done, "aaa", []}, Result).

decode_unquoted_06_test() ->
    String = "aaa,bbb",
    Result = decode_unquoted(String),
    ?assertEqual({go_on, "aaa", "bbb"}, Result).

decode_unquoted_07_test() ->
    String = "abc,bde",
    Result = decode_unquoted(String),
    ?assertEqual({go_on, "abc", "bde"}, Result).
