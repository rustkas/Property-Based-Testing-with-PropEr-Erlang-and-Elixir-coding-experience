decode_quoted_01_test() ->
    Expected = {done, "\"\"", ""},
    String = "\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_02_test() ->
    Expected = {done, "\"\r\n\"", ""},
    String = "\r\n\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_03_test() ->
    Expected = {go_on, "\"\"", "\"\""},
    String = "\",\"\"",
    Result = decode_quoted(String),

    ?assertEqual(Expected, Result).

decode_quoted_04_test() ->
    Expected = {go_on, "\"\"", "\"\r\n\""},
    String = "\",\"\r\n\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_05_test() ->
    Expected = {done, "\"aaa\"", []},
    String = "aaa\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_06_test() ->
    Expected = {go_on, "\"aaa\"", "bbb"},
    String = "aaa\",bbb",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_07_test() ->
    Expected = {done, "\"a\"", "bc"},
    String = "a\"\r\nbc",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_08_test() ->
    Expected = {done, "\"a\"a\"", "bc"},
    String = "a\"\"a\"\r\nbc",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_09_test() ->
    Expected = {go_on, "\"b\r\nbbb\"", "\"ccc\""},
    String = "b\r\nbbb\",\"ccc\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_10_test() ->
    Expected = {done, "\"b\"bbb\"", ""},
    String = "b\"\"bbb\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).

decode_quoted_11_test() ->
    Expected = {done, "\"abcd\"", ""},
    String = "abcd\"",
    Result = decode_quoted(String),
    ?assertEqual(Expected, Result).
