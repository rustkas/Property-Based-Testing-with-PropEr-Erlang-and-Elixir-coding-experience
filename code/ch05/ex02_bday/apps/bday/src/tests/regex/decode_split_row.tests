

research_01_test()->
    SplitMP = build_split_rows_mp(),
    RemoveUselessDoubleQuoteMP = build_remove_double_quote_mp(),

    Text = "\"\n\n\r\n\",",
    List = split_row(Text,SplitMP),
    ?debugFmt("Data List = ~p~n",[List]),
	TunedItemList = remove_useless_double_quote(RemoveUselessDoubleQuoteMP, List),
	?debugFmt("TunedItemList = ~p~n",[TunedItemList]),						
						
						
						
	FullText = "CDB,G\r\n\"\n\n\r\n\",",
	TunedText = tune_string(FullText),
	SplittedText = split_text(TunedText),
	?debugFmt("SplittedText = ~p~n",[SplittedText]),
	
	HeaderList = get_header(SplittedText,SplitMP,RemoveUselessDoubleQuoteMP),
	?debugFmt("HeaderList = ~p~n",[HeaderList]),
	DataString = get_data_string(SplittedText),
	?debugFmt("DataString = ~p~n",[DataString]),
	TrimmedDataString = string:trim(DataString),
	?debugFmt("TrimmedDataString = ~p~n",[TrimmedDataString]),
	DataList = get_data(SplittedText,SplitMP,RemoveUselessDoubleQuoteMP),
	?debugFmt("DataList = ~p~n",[DataList]),
    ok.



